<form action="<%= users_url %>" method= "POST">
  <input type="hidden"
           name="authenticity_token"
           value="<%= form_authenticity_token %>">

  <label> Type your note </label>
  <input type="text"
    name="note[note_text]"
    id='note_text'>
  <br>

  <input type="submit" value="Create Note!">
</form>
